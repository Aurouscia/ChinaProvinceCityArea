<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>ChinaProvinceCityArea</Title>
    <Authors>Aurouscia</Authors>
    <Description>中华人民共和国行政区划数据（精确到区县）</Description>
    <PackageProjectUrl>https://github.com/Aurouscia/ChinaProvinceCityArea</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/Aurouscia/ChinaProvinceCityArea</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>中国;China;province;city;area;district;region</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <Version>0.2.1</Version>
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
